#!/bin/bash
#
#testname:representation in SL(2,Z/pZ), count up to conjugacy
#

list="K3_1:5 K10_7:3 HK6_10:5"

for pair in $list
do
  knot=`echo $pair | cut -f1 -d:`
  p=`echo $pair | cut -f2 -d:`
  echo "Count representations in SL(2,Z/${p}Z) (Kitano-Suzuki) for $knot"
  ./contour --out countsl2zp $p $knot -q
done
